
#include <bits/stdc++.h>
using namespace std;

int q[5], front = -1, rear = -1;

void enqueue(int x) {
    if(rear == 4) {
        cout << "Queue Full\n";
        return;
    }
    if(front == -1) front = 0;
    q[++rear] = x;
}

void dequeue() {
    if(front == -1 || front > rear) {
        cout << "Queue Empty\n";
        return;
    }
    front++;
}

void display() {
    for(int i = front; i <= rear; i++)
        cout << q[i] << " ";
}

int main() {
    enqueue(1);
    enqueue(2);
    enqueue(3);
    dequeue();
    display();
}
